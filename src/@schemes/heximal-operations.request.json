{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "heximal_1": {
      "type": "string",
      "pattern": "^[0-9A-Fa-f]+$"
    },
    "heximal_2": {
      "type": "string",
      "pattern": "^[0-9A-Fa-f]+$"
    },
    "operation_type": {
      "type": "string",
      "enum": [
        "multiply",
        "add"
      ]
    }
  },
  "required": [
    "heximal_1",
    "heximal_2",
    "operation_type"
  ]
}